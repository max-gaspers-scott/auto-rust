version: '3.8'

services:
  db:
    image: postgres:15-alpine
    environment:
      POSTGRES_USER: dbuser
      POSTGRES_PASSWORD: p
      POSTGRES_DB: data
    # ports:
      # - "1111:5432"
    healthcheck:
      test: ["CMD-SHELL", "pg_isready -U dbuser -d data"]
      interval: 5s
      timeout: 5s
      retries: 10


